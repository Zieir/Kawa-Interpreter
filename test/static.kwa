var point a;
var point b;

class point {
  attribute static int x, z;
  attribute int y;
}

main {
  a = new point;
  b = new point;
  a.y = 9;
  a.x = 1;
  a.z = 3;
  b.y = 4;
  b.x = 2;
  b.z = 5;
  print(a.x);   // 2
  print(a.y);   // 9
  print(a.z);   // 5
  print(b.x);   // 2
  print(b.y);   // 4
  print(b.z);   // 5
}
